{
  "name": "storybook-verify",
  "version": "1.0.0",
  "scripts": {
    "cem": "custom-elements-manifest analyze --exclude ./storybook-static",
    "cem:watch": "custom-elements-manifest analyze --exclude ./storybook-static --watch",
    "dev": "vite",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@custom-elements-manifest/analyzer": "^0.10.5",
    "@storybook/addon-a11y": "^9.1.3",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-vitest": "^9.1.3",
    "@storybook/web-components-vite": "^9.1.3",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@wc-toolkit/storybook-helpers": "^9.0.1",
    "chromatic": "^13.1.4",
    "lit": "^3.3.1",
    "playwright": "^1.55.0",
    "storybook": "^9.1.3",
    "storybook-addon-test-codegen": "^2.0.1",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "customElements": "custom-elements.json"
}
